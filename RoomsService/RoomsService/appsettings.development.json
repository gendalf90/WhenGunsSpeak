{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "fileSizeLimitBytes": 104857600,
          "retainedFileCountLimit": 1
        }
      }
    ]
  },
  "DistributedCache": {
    "roomExpirationSeconds": 1,
    "allRoomsExpirationSeconds": 1
  }
}
